.btn {
  padding: 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
  background: white;
}
.btn .left {
  margin-left: auto;
  width: 180rpx;
  height: 68rpx;
  border: 1rpx solid #bfbfbf;
  -webkit-border-radius: 34rpx;
          border-radius: 34rpx;
  font-size: 32rpx;
  font-family: SourceHanSansCN;
  font-weight: 400;
  color: #999999;
  line-height: 68rpx;
  text-align: center;
}
.btn .right {
  margin-left: auto;
  margin-left: 30rpx;
  width: 180rpx;
  height: 68rpx;
  background: -webkit-linear-gradient(300deg, #ffc823, #ffc10c);
  background: linear-gradient(150deg, #ffc823, #ffc10c);
  -webkit-border-radius: 34rpx;
          border-radius: 34rpx;
  font-size: 32rpx;
  font-family: SourceHanSansCN;
  font-weight: 400;
  color: #1a1a1a;
  line-height: 68rpx;
  text-align: center;
}
