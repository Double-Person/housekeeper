.sou {
  width: 100%;
  height: 130rpx;
  background-image: url(data:image/png;base64,);
  overflow: hidden;
}
.sou_ipt {
  width: 671rpx;
  height: 71rpx;
  overflow: hidden;
  margin: 0 auto;
  margin-top: 28rpx;
  -webkit-border-radius: 50rpx;
          border-radius: 50rpx;
  position: relative;
}
input {
  width: 100%;
  height: 71rpx;
  background-color: #fff;
  padding-left: 40rpx;
  position: absolute;
}
.order_txt {
  position: absolute;
  z-index: 2;
  overflow: hidden;
  margin-left: 254rpx;
  margin-top: 19rpx;
}
.sou_icon {
  width: 34rpx;
  height: 35rpx;
  float: left;
}
.sou_icon image {
  width: 100%;
  height: 100%;
}
.order_txt text {
  display: block;
  float: left;
  font-size: 28rpx;
  color: #B2B2B2;
  margin-left: 19rpx;
  margin-top: 2rpx;
}
scroll-view {
  width: 100%;
  height: 110rpx;
  background: #fff;
  overflow: hidden;
}
.top {
  width: 130%;
  height: 110rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: nowrap;
          flex-wrap: nowrap;
  overflow: hidden;
}
.top view {
  height: 61rpx;
  float: left;
  margin-left: 67rpx;
  font-size: 32rpx;
  margin-top: 21rpx;
  line-height: 61rpx;
}
.top view:nth-of-type(1) {
  margin-left: 9rpx;
}
.active {
  border-bottom: 6rpx solid #FFC823;
  font-weight: 700;
}
.form_deta {
  display: none;
}
.block {
  display: block;
}
.state text {
  display: inline-block;
  font-size: 32rpx;
  padding: 19rpx 47rpx;
  border: 1rpx solid #191919;
  margin-top: 20rpx;
  margin-left: 60rpx;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
}
.state text:nth-of-type(1) {
  margin-left: 20rpx;
}
.state .statAct {
  background: #FFC823;
  color: #fff;
  border: none;
}

